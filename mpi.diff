+    	 std::cout << "Status: bad end (Failed!)"    << std::endl;
+    
+    }else{//The program shouldnt come here at all
+    	 std::cout << "Status: none of good nor bad ends happened (Failed!) "    << std::endl;
+    }
+    
+
+    #ifdef VERILATOR_VCD
+    std::cout << "Trace done" << std::endl;
+    tfp->close();
+    #endif
+
+    finalize();
+
+    delete top;
+    exit(0);
+}
